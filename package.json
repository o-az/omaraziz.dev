{
  "name": "omaraziz.dev",
  "exports": "./index.js",
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "preview": "vite preview",
    "clean": "rimraf dist",
    "compile": "cross-env-shell babel src -d dist --source-maps --extensions '.ts'",
    "start": "yarn node-options && node dist/src/index.js",
    "test": "vitest",
    "test-file": "yarn node-options && vitest $npm_config_path",
    "test:nowatch": "vitest --run",
    "lint": "npx eslint --fix ./src ./scripts",
    "format": "npx prettier --write ./src ./scripts",
    "pre-commit": "yarn lint && yarn format && yarn test:nowatch",
    "post-commit": "yarn lint && yarn format && yarn test:nowatch",
    "typecheck": "tsc -p .",
    "gen-sitemap": "yarn tsnode scripts/generate-sitemap.ts && npx prettier --tab-width 2 --write public/sitemap.xml",
    "tsnode": "yarn node-options && ts-node --esm -p --skip-project -T -r tsconfig-paths/register $npm_config_path",
    "node-options": "NODE_OPTIONS=\"--experimental-fetch --no-warnings --experimental-specifier-resolution=node\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@sentry/react": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@prettier/plugin-xml": "^2.2.0",
    "@swc/core": "^1.2.179",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/babel-core": "^6.25.7",
    "@types/eslint": "^8.4.2",
    "@types/node": "^17.0.31",
    "@types/prettier": "^2.6.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/rss": "^0.0.29",
    "@types/tailwindcss": "^3.0.10",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-react": "^1.3.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.0",
    "postcss": "^8.4.13",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "rimraf": "^3.0.2",
    "rss": "^1.2.2",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.7.0",
    "tsc-alias": "^1.6.7",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vite-tsconfig-paths": "^3.3.17",
    "vitest": "^0.10.5"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=17.0.0",
    "yarn": ">=1.22.0"
  },
  "engineStrict": true,
  "version": "1.0.0",
  "description": "My personal website",
  "author": "o-az",
  "license": "GPL-3.0-or-later"
}
