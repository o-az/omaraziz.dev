{
  "name": "omaraziz.dev",
  "exports": "./index.js",
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "preview": "vite preview",
    "clean": "rimraf dist node_modules && yarn install --frozen-lockfile",
    "compile": "cross-env-shell babel src -d dist --source-maps --extensions '.ts'",
    "start": "yarn node-options && node dist/src/index.js",
    "test": "vitest",
    "test-file": "yarn node-options && vitest $npm_config_path",
    "test:nowatch": "vitest --run",
    "lint": "npx eslint --fix ./src ./scripts",
    "format": "npx prettier --write ./src ./scripts",
    "pre-commit": "yarn lint && yarn format && yarn test:nowatch",
    "post-commit": "yarn lint && yarn format && yarn test:nowatch",
    "typecheck": "tsc -p .",
    "gen-sitemap": "yarn tsnode scripts/generate-sitemap.ts && npx prettier --tab-width 2 --write public/sitemap.xml",
    "new-blog": "yarn tsnode scripts/new-blog-post.ts name",
    "tsnode": "yarn node-options && ts-node --esm -p --skip-project -T -r tsconfig-paths/register $npm_config_path",
    "node-options": "NODE_OPTIONS=\"--experimental-fetch --no-warnings --experimental-specifier-resolution=node --trace-deprecation --abort-on-uncaught-exception\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@mdx-js/rollup": "^2.1.1",
    "@sentry/tracing": "^6.19.7",
    "solid-app-router": "^0.3.2",
    "solid-js": "^1.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@prettier/plugin-xml": "^2.2.0",
    "@swc/core": "^1.2.179",
    "@types/babel-core": "^6.25.7",
    "@types/eslint": "^8.4.2",
    "@types/mdx": "^2.0.2",
    "@types/node": "^17.0.31",
    "@types/postcss-load-config": "^3.0.1",
    "@types/prettier": "^2.6.0",
    "@types/rss": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "babel-preset-solid": "^1.4.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rss": "^1.2.2",
    "solid-jsx": "^0.7.1",
    "solid-mdx": "^0.0.6",
    "ts-node": "^10.7.0",
    "tsc-alias": "^1.6.7",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vite-plugin-solid": "^2.2.6",
    "vite-plugin-windicss": "^1.8.4",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.10.5",
    "windicss": "^3.5.3"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=17.0.0",
    "yarn": ">=1.22.0"
  },
  "version": "1.0.0",
  "description": "My personal website",
  "author": "o-az",
  "license": "GPL-3.0-or-later"
}
