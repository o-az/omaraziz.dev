{
  "name": "omaraziz.dev",
  "exports": "./index.js",
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "preview": "vite preview",
    "clean": "rimraf dist",
    "compile": "cross-env-shell babel src -d dist --source-maps --extensions '.ts'",
    "start": "yarn node-options && node dist/src/index.js",
    "test": "yarn node-options && vitest",
    "test-file": "yarn node-options && vitest $npm_config_path",
    "lint": "npx eslint --fix ./src",
    "format": "npx prettier --write ./src",
    "typecheck": "tsc -p .",
    "tsnode": "yarn node-options && ts-node --esm -p --skip-project -T -r tsconfig-paths/register $npm_config_path",
    "node-options": "NODE_OPTIONS=\"--experimental-fetch --no-warnings --experimental-specifier-resolution=node\""
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@swc/core": "^1.2.179",
    "@types/babel-core": "^6.25.7",
    "@types/eslint": "^8.4.2",
    "@types/node": "^17.0.31",
    "@types/prettier": "^2.6.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/tailwindcss": "^3.0.10",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.7",
    "dotenv": "^16.0.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "postcss": "^8.4.13",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.7.0",
    "tsc-alias": "^1.6.7",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vitest": "^0.10.5"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=17.0.0",
    "yarn": ">=1.22.0"
  },
  "engineStrict": true,
  "version": "1.0.0",
