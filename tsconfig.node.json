{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    "target": "ESNext",
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "types": []
  },
  "include": [
    "vite.config.ts",
    "./vite.config.ts",
    "vite.config.*",
    "/vite.config.ts",
    "public/assets/mdx-theme/*.json"
  ],
  "exclude": [
    "node_modules",
    ".scratch",
    "**/node_modules/**",
    "./node_modules",
    "./node_modules/*",
    "./node_modules/vitest/dist/index.d.ts",
    "node_modules/vitest/dist/index.d.ts",
    "node_modules/@antfu/utils/index.d.ts",
    "./node_modules/@antfu/utils/index.d.ts"
  ]
}
